{
    "name": "Tracky",
    "description": "Allow time tracking from the browser with syncing ability thanks to chrome sync storage",
    "version": "0.1.7",
    "minimum_chrome_version": "36",
    "app":{
        "background": {"scripts": ["js/background.js"]}

    },
    "icons": { "16": "img/icon16.png",
        "128": "img/icon128.png" },
    "permissions": [
        "storage"
    ],
    "manifest_version": 2
}
